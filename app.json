{
  "expo": {
    "name": "AppCogniKids",
    "slug": "AppCogniKids",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "appcognikids",
    "deepLinks": [
      "appcognikids://"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.appcognikids"
    },
    "android": {
      "package": "com.appcognikids",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow app-cogni-kids to use your location."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "0de63655-6898-465d-a45c-94ea8177ef33"
      }
    }
  }
}
